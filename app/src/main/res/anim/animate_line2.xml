<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fillBefore="true"
    android:repeatMode="restart"
    android:ordering="sequentially">
    <objectAnimator
        android:duration="1000"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="M120,60 L180,60"
        android:valueTo="M20,60 L80,60"
        android:valueType="pathType"


        app:vc_valueType="pathType" />
    <objectAnimator
        android:duration="1000"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="M20,60 L80,60"
        android:valueTo="M20,150 L80,150"
        android:valueType="pathType"

        app:vc_valueType="pathType" />
    <objectAnimator
        android:duration="1000"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="M20,150 L80,150"
        android:valueTo="M120,150 L180,150"
        android:valueType="pathType"


        app:vc_valueType="pathType" />
    <objectAnimator
        android:duration="1000"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="M120,150 L180,150"
        android:valueTo="M120,60 L180,60"
        android:valueType="pathType"


        app:vc_valueType="pathType" />
</set>