<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fillBefore="true"
    android:repeatMode="restart"
    android:ordering="sequentially">
    <objectAnimator
        android:duration="1000"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="M120,30 L180,30"
        android:valueTo="M20,30 L80,30"
        android:valueType="pathType"


        app:vc_valueType="pathType" />
    <objectAnimator
        android:duration="1000"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="M20,30 L80,30"
        android:valueTo="M20,120 L80,120"
        android:valueType="pathType"


        app:vc_valueType="pathType" />
    <objectAnimator
        android:duration="1000"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="M20,120 L80,120"
        android:valueTo="M120,120 L180,120"
        android:valueType="pathType"


        app:vc_valueType="pathType" />
    <objectAnimator
        android:duration="1000"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="M120,120 L180,120"
        android:valueTo="M120,30 L180,30"
        android:valueType="pathType"

        app:vc_valueType="pathType" />
</set>